{
  "expo": {
    "name": "movilizacion-lideres",
    "slug": "movilizacion-lideres",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "movilizacionlideres",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "jsc",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Red Ciudadana necesita acceso a tu ubicaci贸n para el seguimiento en tiempo real durante movilizaciones.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Red Ciudadana necesita acceso a tu ubicaci贸n para el seguimiento en tiempo real durante movilizaciones."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.anonymous.movilizacionlideres"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Red Ciudadana necesita acceso a tu ubicaci贸n para el seguimiento en tiempo real durante movilizaciones.",
          "locationAlwaysPermission": "Red Ciudadana necesita acceso a tu ubicaci贸n en segundo plano para el seguimiento continuo durante movilizaciones."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
