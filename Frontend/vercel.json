{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://red-ciudadana-production.up.railway.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://red-ciudadana-production.up.railway.app",
    "REACT_APP_ENVIRONMENT": "production",
    "REACT_APP_BACKEND_URL": "https://red-ciudadana-production.up.railway.app"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "build"
}
